{
  "expo": {
    "name": "Reactive Music",
    "slug": "reactive-music",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/img/reactive-icon.png",
    "splash": {
      "image": "./src/assets/img/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/img/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.reactivemusic"
    },
    "web": {
      "favicon": "./src/assets/img/favicon.png"
    }
  }
}
